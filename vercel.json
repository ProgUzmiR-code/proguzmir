{
  "version": 2,
  "functions": {
    "save.js": {
      "runtime": "nodejs18.x"
    },
    "load.js": {
      "runtime": "nodejs18.x"
    }
  },
  "storage": {
    "programt-blob": {
      "driver": "blob"
    }
  },
  "builds": [
    { "src": "game/*", "use": "@vercel/static" },
    { "src": "image/*", "use": "@vercel/static" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/game/(.*)", "dest": "/game/$1" },
    { "src": "/image/(.*)", "dest": "/image/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}