{
  "version": 2,
  "builds": [
    { "src": "game/*.html", "use": "@vercel/static" },
    { "src": "game/*.js", "use": "@vercel/static" },
    { "src": "game/*.*(png|jpg|jpeg|svg|gif)", "use": "@vercel/static" },
    { "src": "image/*", "use": "@vercel/static" },
    { "src": "/*.*(png|jpg|jpeg|svg|gif)", "use": "@vercel/static" },
    { "src": "/*.html", "use": "@vercel/static" },
    { "src": "/*.js", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/game/(.*)", "dest": "/game/$1" },
    { "src": "/image/(.*)", "dest": "/image/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}