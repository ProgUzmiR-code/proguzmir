{
  "version": 2,
  "buildCommand": "npm install",
  "env": {
    "BOT_TOKEN": "@BOT_TOKEN"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "game/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "game/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "rank/**/*.{html,js}",
      "use": "@vercel/static"
    },
    {
      "src": "earn/**/*.{html,js}",
      "use": "@vercel/static"
    },
    {
      "src": "wallet/**/*.{html,js}",
      "use": "@vercel/static"
    },
    {
      "src": "friends/**/*.{html,js}",
      "use": "@vercel/static"
    },
    {
      "src": "lusck_code/**/*.{html,js}",
      "use": "@vercel/static"
    },
    {
      "src": "image/**",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "index.js",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/server",
      "dest": "/server.js"
    },
    {
      "src": "/game/(.*)",
      "dest": "/game/$1"
    },
    {
      "src": "/rank/(.*)",
      "dest": "/rank/$1"
    },
    {
      "src": "/earn/(.*)",
      "dest": "/earn/$1"
    },
    {
      "src": "/wallet/(.*)",
      "dest": "/wallet/$1"
    },
    {
      "src": "/friends/(.*)",
      "dest": "/friends/$1"
    },
    {
      "src": "/lusck_code/(.*)",
      "dest": "/lusck_code/$1"
    },
    {
      "src": "/image/(.*)",
      "dest": "/image/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}