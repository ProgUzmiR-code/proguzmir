{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    { "src": "/api/index", "dest": "api/index.py" },
    { "src": "/server", "dest": "/server.js" },
    { "src": "/game/(.*)", "dest": "/game/$1" },
    { "src": "/rank/(.*)", "dest": "/rank/$1" },
    { "src": "/earn/(.*)", "dest": "/earn/$1" },
    { "src": "/friends/(.*)", "dest": "/friends/$1" },
    { "src": "/wallet/(.*)", "dest": "/wallet/$1" },
    { "src": "/lusck_code/(.*)", "dest": "/lusck_code/$1" },
    { "src": "/image/(.*)", "dest": "/image/$1" },
    { "src": "/api/coin", "dest": "api/coin.png" },
    { "src": "/", "dest": "/index.html" }
  ]
}